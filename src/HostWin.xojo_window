#tag WindowCode
	#tag Method, Flags = &h0
		Function GetCapabilities(url as String) As boolean
		  dim https as New HTTPSecureSocket
		  dim data as string
		  dim node as JSONNode
		  dim nodeArray as JSONNode
		  dim cNode as JSONNode
		  dim ciNode as JSONNode
		  
		  https.Secure = cbSSL.value // True // sets port to 443
		  https.ConnectionType = SSLSocket.TLSv12
		  https.SetRequestHeader("User-Agent:","RandomThingsUtility")
		  
		  data = https.Get(url,5)
		  
		  if https.ErrorCode>1 then
		    
		    MsgBox "HTTP Error while fetching data."+EndOfLine+"Error code "+str(https.ErrorCode)
		    
		  end
		  
		  node = JSONNode.Parse(data)
		  nodeArray = node.Child("value")
		  
		  if nodeArray<>Nil then
		    
		    lbCapabilities.DeleteAllRows
		    
		    for i as integer = 0 to nodeArray.ChildCount-1
		      
		      cNode = nodeArray.Child(i)
		      if cNode<>nil then
		        ciNode = cNode.Child("name")
		        if ciNode<>Nil then
		          lbCapabilities.addrow ciNode.StringValue
		        end
		      end
		    next
		    
		  end
		  
		  return true
		  
		  // ====== EXAMPLE DATA ===========
		  
		  // {
		  // -"value": [
		  // -{
		  // "name": "Things",
		  // "url": "https://toronto-bike-snapshot.sensorup.com/v1.0/Things"
		  // },
		  // -{
		  // "name": "Locations",
		  // "url": "https://toronto-bike-snapshot.sensorup.com/v1.0/Locations"
		  // },
		  // -{
		  // "name": "HistoricalLocations",
		  // "url": "https://toronto-bike-snapshot.sensorup.com/v1.0/HistoricalLocations"
		  // },
		  // -{
		  // "name": "Datastreams",
		  // "url": "https://toronto-bike-snapshot.sensorup.com/v1.0/Datastreams"
		  // },
		  // -{
		  // "name": "Sensors",
		  // "url": "https://toronto-bike-snapshot.sensorup.com/v1.0/Sensors"
		  // },
		  // -{
		  // "name": "Observations",
		  // "url": "https://toronto-bike-snapshot.sensorup.com/v1.0/Observations"
		  // },
		  // -{
		  // "name": "ObservedProperties",
		  // "url": "https://toronto-bike-snapshot.sensorup.com/v1.0/ObservedProperties"
		  // },
		  // -{
		  // "name": "FeaturesOfInterest",
		  // "url": "https://toronto-bike-snapshot.sensorup.com/v1.0/FeaturesOfInterest"
		  // }
		  // ]
		  // }
		End Function
	#tag EndMethod


